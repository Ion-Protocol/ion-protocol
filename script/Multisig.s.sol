pragma solidity >=0.8.19 <=0.9.0;

import { ICreateX } from "../src/interfaces/ICreateX.sol";
import { CREATEX } from "../src/Constants.sol";

import { BaseScript } from "./Base.s.sol";

contract DeployScript is BaseScript {
    function run() public broadcast returns (address deployed) {
        bytes memory gnosisSafeProxyBytecode = hex"";

        address singleton = 0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552;

        bytes memory initCode = abi.encodePacked(gnosisSafeProxyBytecode, abi.encode(singleton));

        bytes memory initData =
            hex"";

        bytes32 salt = 0x0000000000000000000000000000000000000000010aab6e0df381e700b7d793;

        ICreateX.Values memory values;

        deployed = CREATEX.deployCreate3AndInit(salt, initCode, initData, values);
    }
}
